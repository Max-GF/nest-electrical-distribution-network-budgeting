{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "infra/main",
  "compilerOptions": {
    "assets": [
      {
        "include": "../prisma/*",
        "outDir": "./dist/prisma",
        "watchAssets": true
      }
    ],
    "deleteOutDir": true
  }
}
